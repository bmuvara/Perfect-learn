Pipeline{
    agent any
    stages{
        stage("checkout"){
                steps{
                        git 'https://github.com/bmuvara/Perfect-learn.git'
                      }
                }
  
        stage("Test"){
                steps{
                       sh 'sudo apt npm install'
                       sh  'npm test'

                      }
                }
        
        stage("Build"){
                steps{
                       sh 'npm run build'
                    
                      }
                }

   
